<launch>
<arg name="ns" default="J0" />
<node ns="$(arg ns)" pkg="simple_frontier" type="explore" respawn="false" name="explore" output="screen">
  <param name="robot_base_frame" value="$(arg ns)/base_link"/>
  <param name="costmap_topic" value="/$(arg ns)/locus/octree_projected_map"/>
  <param name="visualize" value="true"/>
  <param name="planner_frequency" value="10"/>
  <param name="progress_timeout" value="30.0"/>
  <param name="potential_scale" value="3.0"/>
  <param name="orientation_scale" value="0.0"/>
  <param name="gain_scale" value="1.0"/>
  <param name="transform_tolerance" value="0.3"/>
  <param name="min_frontier_size" value="0.5"/>
</node>
</launch>
